<ContagemNumerosScreen>:
    name: 'contagem_numeros_screen'
    BoxLayout:
        name: 'layout_contagem_screen'
        orientation: 'vertical'
        ActionBar:
            background_color: app.primary_color
            ActionView:
                ActionPrevious:
                    title: 'Contagem de Números'
                    app_icon: ''
                    with_previous: False
                    color: app.secundary_color
                ActionButton:
                    text: 'Voltar'
                    color: app.secundary_color
                    on_release: root.manager.current = 'menu_screen'        
        Label:
            text: 'Contagem'                    
