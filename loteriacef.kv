<ResultadosScreen>:
    name: 'resultados_screen'
    GridLayout:
        name: 'layout_resultados_screen'
        rows: 3
        ActionBar:
            background_color: app.primary_color
            ActionView:
                ActionPrevious:
                    title: ''
                    app_icon: ''
                    with_previous: False
                    color: app.secundary_color
                ActionButton:
                    text: 'Voltar'
                    color: app.secundary_color
                    on_release: root.manager.current = 'menu_screen'
        GridLayout:
            cols: 4
            rows: 1
            size_hint: 1, .2
            MDLabel:
                id: lbl_resultados_sorteio
                text: 'Sorteio:'
                font_size: 24
                color: app.primary_color
                size_hint: .2, 1
                padding: '20dp'    
            MDTextField:
                id: txt_resultados_sorteio
                label_id: 'lbl_resultados_sorteio'
                text: ''          
                font_size: 32
                halign: 'center'
                size_hint: .5, 1
                input_filter: 'int'
                required: True
                helper_text: 'Informe o sorteio'
                helper_text_mode: 'on_error'
                icon_left_color_normal: app.primary_color
                icon_left_color_focus: app.primary_color
                line_color_normal: app.primary_color
                line_color_focus: app.primary_color
                text_color_normal: app.primary_color
                text_color_focus: app.primary_color
